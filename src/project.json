{
    "version": "1.0.0-*",
    "buildOptions": {
        "emitEntryPoint": true,
        "outputName": "dotnet-make"
    },
    "dependencies": {
        "NETStandard.Library": "1.6.0"
    },
    "frameworks": {
        "netcoreapp1.0": {
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.0.0"
                }
            },
            "imports": "dnxcore50"
        }
    },
    "packOptions": {
        "summary": "A dotnet core sample tool that wraps command-lines and prevent incremental compilation.",
        "tags": [
            "netcore",
            "tool"
        ],
        "projectUrl": "https://github.com/springcomp/dotnet-make",
        "licenseUrl": "https://github.com/springcomp/dotnet-make/blob/master/LICENSE",
        "requireLicenseAcceptance": false
    }
}